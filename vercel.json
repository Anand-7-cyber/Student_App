{
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/public/auth.html" },
    { "src": "/about", "dest": "/public/about.html" },
    { "src": "/dashboard", "dest": "/public/dashboard.html" },
    { "src": "/ebooks", "dest": "/public/ebooks.html" }
  ]
}
