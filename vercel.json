{
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/auth.html"
    },
    {
      "src": "/about",
      "dest": "/about.html"
    },
    {
      "src": "/dashboard",
      "dest": "/dashboard.html"
    },
    {
      "src": "/reset",
      "dest": "/reset.html"
    },
    {
      "src": "/ebooks",
      "dest": "/ebooks.html"
    },
    {
      "src": "/forgot",
      "dest": "/forgot.html"
    }
  ]
}
